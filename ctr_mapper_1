"""mapper 1"""
import sys
for line in sys.stdin:
    line = line.strip()
    stats = line.split('\t')
    usertxt = open('userid_profile.txt')
    instancestxt = open('instances.txt')
    #if user file entered
    if len(stats) == 3:
        User = stats[0]
        Age = stats[2]
        for line in instancestxt:
            line = line.strip()
            info = line.split('\t')
            if User == info[11]:
                Clicks = info[0]
                Impress = info[1]
                print '%s\t%s\t%s' % (Age, Clicks, Impress)
    #if instances file entered
    else:
        User = stats[11]
        Clicks = stats[0]
        Impress = stats[1]
        for line in usertxt:
            line = line.strip()
            info = line.split('\t')
            if User == info[0]:
                Age = info[2]
                print '%s\t%s\t%s' % (Age, Clicks, Impress)
