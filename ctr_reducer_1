"""reducer 1"""
import sys

CLICK_SUM = 0
IMP_SUM = 0
CURR_AGE = None

for line in sys.stdin:
    line.strip()
    age, clicks, impress = line.split('\t')
    if CURR_AGE == age:
        CLICK_SUM = int(CLICK_SUM) + int(clicks)
        IMP_SUM = int(IMP_SUM) + int(impress)
        CURR_AGE = age
    else:
        if CURR_AGE:
            print '%s\t%s\t%s' % (CURR_AGE, CLICK_SUM, IMP_SUM)
        CLICK_SUM = clicks
        IMP_SUM = impress
if CURR_AGE == age:
    print '%s\t%s\t%s' % (CURR_AGE, CLICK_SUM, IMP_SUM)
